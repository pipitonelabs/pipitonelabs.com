<?xml version="1.0" encoding="UTF-8"?>
<svg width="1200" height="630" viewBox="0 0 1200 630" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <!-- Gradient for background -->
    <linearGradient id="bgGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#1a5fb4;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#4a90e2;stop-opacity:1" />
    </linearGradient>
    
    <!-- Glow filter -->
    <filter id="glow" x="-50%" y="-50%" width="200%" height="200%">
      <feGaussianBlur stdDeviation="8" result="blur"/>
      <feComposite in="SourceGraphic" in2="blur" operator="over"/>
    </filter>
    
    <!-- Subtle grid pattern -->
    <pattern id="grid" width="40" height="40" patternUnits="userSpaceOnUse">
      <path d="M 40 0 L 0 0 0 40" fill="none" stroke="#ffffff" stroke-width="0.5" opacity="0.2"/>
    </pattern>
  </defs>

  <!-- Background -->
  <rect width="1200" height="630" fill="url(#bgGradient)"/>
  <rect width="1200" height="630" fill="url(#grid)"/>
  
  <!-- Main title -->
  <text x="600" y="120" font-family="Arial, sans-serif" font-size="48" font-weight="bold" text-anchor="middle" fill="white">
    Agentic AI
  </text>
  <text x="600" y="160" font-family="Arial, sans-serif" font-size="36" font-weight="bold" text-anchor="middle" fill="white">
    Experiments
  </text>
  
  <!-- Subtitle -->
  <text x="600" y="220" font-family="Arial, sans-serif" font-size="24" text-anchor="middle" fill="#e8f4fd">
    Exploring Cline, Kilo Code & Antigravity
  </text>
  
  <!-- Central AI representation -->
  <g transform="translate(600, 350)">
    <!-- Main circle representing AI brain -->
    <circle cx="0" cy="0" r="80" fill="white" opacity="0.15" filter="url(#glow)"/>
    
    <!-- Neural network nodes -->
    <circle cx="-60" cy="-40" r="15" fill="#ffffff" opacity="0.8"/>
    <circle cx="60" cy="-40" r="15" fill="#ffffff" opacity="0.8"/>
    <circle cx="-80" cy="0" r="15" fill="#ffffff" opacity="0.6"/>
    <circle cx="80" cy="0" r="15" fill="#ffffff" opacity="0.6"/>
    <circle cx="-60" cy="40" r="15" fill="#ffffff" opacity="0.8"/>
    <circle cx="60" cy="40" r="15" fill="#ffffff" opacity="0.8"/>
    <circle cx="0" cy="-60" r="15" fill="#ffffff" opacity="0.7"/>
    <circle cx="0" cy="60" r="15" fill="#ffffff" opacity="0.7"/>
    
    <!-- Central processing unit -->
    <circle cx="0" cy="0" r="25" fill="#ffffff" opacity="0.9"/>
    <text x="0" y="8" font-family="Arial, sans-serif" font-size="16" font-weight="bold" text-anchor="middle" fill="#1a5fb4">AI</text>
    
    <!-- Connecting lines -->
    <line x1="-60" y1="-40" x2="0" y2="0" stroke="white" stroke-width="2" opacity="0.4"/>
    <line x1="60" y1="-40" x2="0" y2="0" stroke="white" stroke-width="2" opacity="0.4"/>
    <line x1="-80" y1="0" x2="0" y2="0" stroke="white" stroke-width="2" opacity="0.4"/>
    <line x1="80" y1="0" x2="0" y2="0" stroke="white" stroke-width="2" opacity="0.4"/>
    <line x1="-60" y1="40" x2="0" y2="0" stroke="white" stroke-width="2" opacity="0.4"/>
    <line x1="60" y1="40" x2="0" y2="0" stroke="white" stroke-width="2" opacity="0.4"/>
    <line x1="0" y1="-60" x2="0" y2="0" stroke="white" stroke-width="2" opacity="0.4"/>
    <line x1="0" y1="60" x2="0" y2="0" stroke="white" stroke-width="2" opacity="0.4"/>
  </g>
  
  <!-- Tool representations -->
  <g transform="translate(300, 480)">
    <rect x="-40" y="-20" width="80" height="40" rx="8" fill="white" opacity="0.8"/>
    <text x="0" y="8" font-family="Arial, sans-serif" font-size="14" font-weight="bold" text-anchor="middle" fill="#1a5fb4">Cline</text>
  </g>
  
  <g transform="translate(600, 480)">
    <rect x="-40" y="-20" width="80" height="40" rx="8" fill="white" opacity="0.8"/>
    <text x="0" y="8" font-family="Arial, sans-serif" font-size="14" font-weight="bold" text-anchor="middle" fill="#1a5fb4">Kilo Code</text>
  </g>
  
  <g transform="translate(900, 480)">
    <rect x="-40" y="-20" width="80" height="40" rx="8" fill="white" opacity="0.8"/>
    <text x="0" y="8" font-family="Arial, sans-serif" font-size="14" font-weight="bold" text-anchor="middle" fill="#1a5fb4">Antigravity</text>
  </g>
  
  <!-- Footer -->
  <text x="600" y="590" font-family="Arial, sans-serif" font-size="16" text-anchor="middle" fill="#e8f4fd">
    The Future of IT Work is Here
  </text>
</svg>
